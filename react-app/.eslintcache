  localProvider,\n  mainnetProvider,\n  price,\n  minimized,\n  web3Modal,\n  loadWeb3Modal,\n  logoutOfWeb3Modal,\n  blockExplorer,\n  isContract,\n}) {\n  const { currentTheme } = useThemeSwitcher();\n\n  let accountButtonInfo;\n  if (web3Modal?.cachedProvider) {\n    accountButtonInfo = { name: 'Logout', action: logoutOfWeb3Modal };\n  } else {\n    accountButtonInfo = { name: 'Connect', action: loadWeb3Modal };\n  }\n\n  const display = !minimized && (\n    <span>\n      {address && (\n        <Address\n          address={address}\n          ensProvider={mainnetProvider}\n          blockExplorer={blockExplorer}\n          fontSize={20}\n        />\n      )}\n      <Balance address={address} provider={localProvider} price={price} size={20} />\n      {!isContract && (\n        <Wallet\n          address={address}\n          provider={localProvider}\n          signer={userSigner}\n          ensProvider={mainnetProvider}\n          price={price}\n          color={currentTheme === \"light\" ? \"#1890ff\" : \"#2caad9\"}\n          size={22}\n          padding={\"0px\"}\n        />\n      )}\n    </span>\n  );\n\n  return (\n    <div style={{ display: \"flex\" }}>\n      {display}\n      {web3Modal && (\n        <Button\n          style={{ marginLeft: 8 }}\n          shape=\"round\"\n          onClick={accountButtonInfo.action}\n        >\n          {accountButtonInfo.name}\n        </Button>\n      )}\n    </div>\n  );\n}\n","/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Address.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Blockie.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/EtherInput.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/GasGauge.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Faucet.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Ramp.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/ThemeSwitch.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Timeline.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/TokenBalance.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Header.jsx",["220","221","222"],"import React from \"react\";\nimport { Typography } from \"antd\";\n\nconst { Title, Text } = Typography;\n\n// displays a page header\n\nexport default function Header({ link, title, subTitle, ...props }) {\n  return (\n    <div style={{ display: \"flex\", justifyContent: \"space-between\", padding: \"1.2rem\" }}>\n      <div style={{ display: \"flex\",  flexDirection: \"column\", flex: 1, alignItems: \"start\" }}>\n        <a href={link} target=\"_blank\" rel=\"noopener noreferrer\">\n          <Title level={4} style={{ margin: \"0 0.5rem 0 0\" }}>{title}</Title>\n        </a>\n        <Text type=\"secondary\" style={{ textAlign: \"left\" }}>{subTitle}</Text>\n      </div>\n      {props.children}\n    </div>\n  );\n}\n\nHeader.defaultProps = {\n  link: \"https://github.com/austintgriffith/scaffold-eth\",\n  title: \"🏗 scaffold-eth\",\n  subTitle: \"forkable Ethereum dev stack focused on fast product iteration\",\n};\n","/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/FaucetHint.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Wallet.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/L2Bridge.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Swap.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/NetworkSwitch.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/MultiAddressInput.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/NetworkDisplay.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/TokenSelect.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/BytesStringInput.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Balance.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Provider.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Events.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/views/Subgraph.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/views/Hints.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/views/ExampleUI.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/views/Home.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/helpers/loadAppContracts.js",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Contract/index.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Contract/FunctionForm.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Contract/DisplayVariable.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/components/Contract/utils.jsx",[],"/Users/sampink/repos/eth-nyc/scaffold-eth/packages/react-app/src/views/createStream.jsx",["223","224","225","226"],"//import { Button, Card, DatePicker, Divider, Input, Progress, Slider, Spin, Switch, Row, Col } from \"antd\";\nimport React, { useState } from \"react\";\n\nimport \"bootstrap/dist/css/bootstrap.min.css\";\n\nimport { Card, Row, Col, Button, Alert, Form, InputGroup } from \"react-bootstrap\";\n\nimport { Address, Balance, Events, AddressInput } from \"../components\";\n\nimport { INFURA_ID } from \"../constants\";\n\nimport { ethers } from \"ethers\";\n\nimport { Framework } from \"@superfluid-finance/sdk-core\";\n\n// Ethers.js provider initialization\nconst customHttpProvider = new ethers.providers.InfuraProvider(\"rinkeby\", INFURA_ID);\n\n//where the Superfluid logic takes place\nasync function createNewFlow(recipient, flowRate, provider) {\n  const sf = await Framework.create({\n    chainId: 4,\n    provider: provider,\n  });\n\n  const signer = sf.createSigner({\n    privateKey: \"ae4b6f52bfbdeadbca2901f6776c0411bdff7b482192fb6743d755a52a1995c3\",\n    provider: provider,\n  });\n\n  const ETHxContraactAddress = await sf.loadSuperToken(\"ETHx\");\n  const ETHx = ETHxContraactAddress.address;\n\n  try {\n    const createFlowOperation = sf.cfaV1.createFlow({\n      flowRate: flowRate,\n      receiver: recipient,\n      superToken: ETHx,\n      gasLimit: 1000000000000,\n    });\n\n    console.log(\"Creating your stream...\");\n\n    const result = await createFlowOperation.exec(signer);\n    console.log(result);\n\n    console.log(\n      `Congrats - you've just created a money stream!\n    View Your Stream At: https://app.superfluid.finance/dashboard/${recipient}\n    Network: Goerli\n    Super Token: DAIx\n    Sender: 0xDCB45e4f6762C3D7C61a00e96Fb94ADb7Cf27721\n    Receiver: ${recipient},\n    FlowRate: ${flowRate}\n    `,\n    );\n  } catch (error) {\n    console.log(\n      \"Hmmm, your transaction threw an error. Make sure that this stream does not already exist, and that you've entered a valid Ethereum address!\",\n    );\n    console.error(error);\n  }\n}\n\n//create a page that says hey\nexport default function CreateStream({ price, address, readContracts, mainnetProvider, sf }) {\n  const [toAddress, setToAddress] = useState(\"\");\n  const [flowRate, setFlowRate] = useState(\"\");\n  const [flowRateDisplay, setFlowRateDisplay] = useState(\"\");\n\n  function calculateFlowRate(amount) {\n    if (typeof Number(amount) !== \"number\" || isNaN(Number(amount)) === true) {\n      alert(\"You can only calculate a flowRate based on a number\");\n      return;\n    } else if (typeof Number(amount) === \"number\") {\n      if (Number(amount) === 0) {\n        return 0;\n      }\n      const amountInWei = ethers.BigNumber.from(amount);\n      const monthlyAmount = ethers.utils.formatEther(amountInWei.toString());\n      const calculatedFlowRate = monthlyAmount * 3600 * 24 * 30;\n      return calculatedFlowRate;\n    }\n  }\n\n  const handleFlowRateChange = e => {\n    setFlowRate(() => ([e.target.name] = e.target.value));\n    // if (typeof Number(flowRate) === \"number\") {\n    let newFlowRateDisplay = calculateFlowRate(e.target.value);\n    setFlowRateDisplay(newFlowRateDisplay.toString());\n    // setFlowRateDisplay(() => calculateFlowRate(e.target.value));\n    // }\n  };\n\n  return (\n    <div>\n      <Row xs={1} md={2} className=\"g-4\">\n        {Array.from({ length: 1 }).map((_, idx) => (\n          <Col>\n            <Card>\n              <Card.Header>\n                <Card.Title>Stream {idx + 1}</Card.Title>\n              </Card.Header>\n              <Card.Body>\n                <Card.Title className=\"d-flex justify-content-start\"></Card.Title>\n                <div style={{ width: 350, padding: 16, margin: \"auto\" }}>\n                  <h5>Create a new stream to this address:</h5>\n                  <AddressInput ensProvider={mainnetProvider} value={toAddress} onChange={setToAddress} />\n                  <h5>With this flow rate:</h5>\n                  <InputGroup size=\"sm\" className=\"mb-3\">\n                    <InputGroup.Text id=\"inputGroup-sizing-sm\">Small</InputGroup.Text>\n                    <Form.Control\n                      aria-label=\"Small\"\n                      aria-describedby=\"inputGroup-sizing-sm\"\n                      name=\"FlowRate\"\n                      value={flowRate}\n                      onChange={handleFlowRateChange}\n                    />\n                  </InputGroup>\n                  <p>Your flow will be equal to:</p>\n                  <p>\n                    <b>${flow